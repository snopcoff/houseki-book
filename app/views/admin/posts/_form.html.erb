<div>
  <%= form_for [:admin, @post], html: {multipart: true} do |f| %>
    <%= render "shared/error_messages", object: @post %>
    <div class="field">

      <%= f.label "Post content", class: "control-laber" %>
      <%= f.text_field :content, placeholder: "Your content",
        class: "form-control" %>

    </div>
    <%= f.submit t("books.new.save"), class: "btn btn-primary" %>
  <% end %>
</div>
